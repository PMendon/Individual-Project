/*import dataset*/
PROC IMPORT datafile='C:\Users\pmendon\Downloads\ashline.csv' out=ashline dbms=csv replace; 
	getnames=yes; 
RUN; 

PROC CONTENTS data=ashline; 
RUN; 

/*create #Comorbid Conditions Variable*/ 
DATA ashline2; 
	set ashline; 
	comorbid=0; 
	IF asthma=1 then comorbid=comorbid+1; 
	IF cancer=1 then comorbid=comorbid+1; 
	IF copd=1 then comorbid=comorbid+1; 
	IF diabetes=1 then comorbid=comorbid+1; 
	IF heartdisease=1 then comorbid=comorbid+1; 
	IF hypertension=1 then comorbid=comorbid+1;  
RUN; 

PROC PRINT data=ashline2; 
	var asthma cancer copd diabetes heartdisease hypertension comorbid;
RUN; 
